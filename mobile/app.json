{
  "expo": {
    "name": "Happy",
    "slug": "happy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0089A5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.happymobileapp.happyapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.happymobileapp.happyapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0089A5"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
